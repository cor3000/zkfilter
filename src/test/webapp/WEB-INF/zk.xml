<?xml version="1.0" encoding="UTF-8"?>

<zk>
	<!-- 
	Enable JavaScript Debugging	
	-->
	<client-config>
		<debug-js>true</debug-js>
	</client-config>
	
	<session-config>
		<device-type>ajax</device-type>
		<automatic-timeout/>
		<timeout-uri />		
	</session-config>
	
	<library-property>
	    <name>org.zkoss.zul.grid.rod</name>
	    <value>true</value>
	</library-property>
	<library-property>
	    <name>org.zkoss.zul.listbox.rod</name>
	    <value>true</value>
	</library-property>
</zk>
